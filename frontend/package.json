{
  "name": "medical-ai-agent-frontend",
  "version": "1.0.0",
  "description": "医疗智能体 - 微信小程序前端",
  "main": "main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "dev:h5": "uni --platform h5 --mode development",
    "build:h5": "uni build --platform h5 --mode production",
    "dev:h5:ssr": "UNI_PLATFORM=h5 UNI_SSR=true vite",
    "dev:mp-alipay": "UNI_PLATFORM=mp-alipay vite",
    "dev:mp-baidu": "UNI_PLATFORM=mp-baidu vite",
    "dev:mp-kuaishou": "UNI_PLATFORM=mp-kuaishou vite",
    "dev:mp-lark": "UNI_PLATFORM=mp-lark vite",
    "dev:mp-qq": "UNI_PLATFORM=mp-qq vite",
    "dev:mp-toutiao": "UNI_PLATFORM=mp-toutiao vite",
    "dev:mp-weixin": "UNI_PLATFORM=mp-weixin vite",
    "dev:mp-weixin-fixed": "uni --platform mp-weixin --mode production && node fix-tabbar-icons.js",
    "dev:mp-xhs": "UNI_PLATFORM=mp-xhs vite",
    "dev:quickapp-webview": "UNI_PLATFORM=quickapp-webview vite",
    "build:app": "UNI_PLATFORM=app-plus vite build",
    "build:mp-alipay": "UNI_PLATFORM=mp-alipay vite build",
    "build:mp-baidu": "UNI_PLATFORM=mp-baidu vite build",
    "build:mp-kuaishou": "UNI_PLATFORM=mp-kuaishou vite build",
    "build:mp-lark": "UNI_PLATFORM=mp-lark vite build",
    "build:mp-qq": "UNI_PLATFORM=mp-qq vite build",
    "build:mp-toutiao": "UNI_PLATFORM=mp-toutiao vite build",
    "build:mp-weixin": "uni build -p mp-weixin --mode production",
    "build:mp-xhs": "UNI_PLATFORM=mp-xhs vite build",
    "build:quickapp-webview": "UNI_PLATFORM=quickapp-webview vite build",
    "type-check": "vue-tsc --noEmit",
    "fix-tabbar": "node fix-tabbar-icons.js",
    "debug": "NODE_ENV=development UNI_PLATFORM=mp-weixin vite --mode development",
    "debug:h5": "NODE_ENV=development UNI_PLATFORM=h5 vite --mode development",
    "debug:mp-weixin": "NODE_ENV=development UNI_PLATFORM=mp-weixin vite --mode development --force",
    "clean": "rm -rf dist && rm -rf node_modules/.vite",
    "restart": "npm run clean && npm install && npm run debug:mp-weixin"
  },
  "dependencies": {
    "@dcloudio/uni-app": "3.0.0-4070520250711001",
    "@dcloudio/uni-app-harmony": "3.0.0-4070520250711001",
    "@dcloudio/uni-app-plus": "3.0.0-4070520250711001",
    "@dcloudio/uni-components": "3.0.0-4070520250711001",
    "@dcloudio/uni-h5": "3.0.0-4070520250711001",
    "@dcloudio/uni-mp-alipay": "3.0.0-4070520250711001",
    "@dcloudio/uni-mp-baidu": "3.0.0-4070520250711001",
    "@dcloudio/uni-mp-harmony": "3.0.0-4070520250711001",
    "@dcloudio/uni-mp-jd": "3.0.0-4070520250711001",
    "@dcloudio/uni-mp-kuaishou": "3.0.0-4070520250711001",
    "@dcloudio/uni-mp-lark": "3.0.0-4070520250711001",
    "@dcloudio/uni-mp-qq": "3.0.0-4070520250711001",
    "@dcloudio/uni-mp-toutiao": "3.0.0-4070520250711001",
    "@dcloudio/uni-mp-weixin": "3.0.0-4070520250711001",
    "@dcloudio/uni-mp-xhs": "3.0.0-4070520250711001",
    "@dcloudio/uni-quickapp-webview": "3.0.0-4070520250711001",
    "@dcloudio/uni-ui": "^1.5.8",
    "dayjs": "^1.11.10",
    "pinia": "2.0.36",
    "uview-plus": "^3.4.45",
    "vue": "3.5.18",
    "vue-i18n": "9.14.5"
  },
  "devDependencies": {
    "@dcloudio/types": "3.4.19",
    "@dcloudio/uni-automator": "3.0.0-4070520250711001",
    "@dcloudio/uni-cli-i18n": "^2.0.2-4060620250520001",
    "@dcloudio/uni-cli-shared": "3.0.0-4070520250711001",
    "@dcloudio/uni-stacktracey": "3.0.0-4070520250711001",
    "@dcloudio/vite-plugin-uni": "3.0.0-4070520250711001",
    "@vue/runtime-core": "3.5.18",
    "@vue/tsconfig": "^0.5.1",
    "sass": "^1.89.2",
    "typescript": "^5.2.2",
    "vite": "5.2.8",
    "vue-tsc": "^1.8.27",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  },
  "browserslist": [
    "Android >= 4.4",
    "ios >= 9"
  ],
  "uni-app": {
    "scripts": {}
  },
  "keywords": [
    "uniapp",
    "vue3",
    "typescript",
    "medical",
    "ai",
    "healthcare",
    "wechat"
  ],
  "author": "Medical AI Agent Team",
  "license": "MIT"
}
